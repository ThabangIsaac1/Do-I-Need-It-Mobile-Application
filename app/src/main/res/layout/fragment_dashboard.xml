<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black">


    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="360dp"
        android:layout_height="213dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginEnd="17dp"
        android:layout_marginRight="17dp"
        app:srcCompat="@drawable/handset"
        android:contentDescription="@string/image"/>

    <ImageView
        android:id="@+id/usericon"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:layout_marginStart="283.3dp"
        android:layout_marginLeft="283.3dp"
        android:layout_marginTop="37.3dp"
        android:layout_marginEnd="64.7dp"
        android:layout_marginRight="64.7dp"
        android:layout_marginBottom="596.7dp"
        android:contentDescription="@string/image"
        app:srcCompat="@drawable/user" />

    <TextView
        android:layout_width="108.7dp"
        android:layout_height="28dp"
        android:layout_marginTop="31.3dp"
        android:layout_marginBottom="582.7dp"
        android:layout_marginLeft="27.7dp"
        android:layout_marginRight="229.7dp"
        android:fontFamily="@font/poppinssemibold"
        android:gravity="center"
        android:text="@string/dashboard"
        android:textColor="@color/fuel_orange"
        android:textSize="15sp"
        android:layout_marginEnd="229.7dp"
        android:layout_marginStart="27.7dp" />

    <TextView
        android:id="@+id/displayusername"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="301.3dp"
        android:layout_marginRight="21dp"
        android:layout_marginBottom="596.7dp"
        android:layout_marginTop="33.3dp"
        android:text="@string/thabang"
        android:layout_marginStart="301.3dp"
        android:layout_marginEnd="21dp"
        android:fontFamily="@font/poppinssemibold"
        android:gravity="center"
        android:textColor="@color/white"
        tools:ignore="RelativeOverlap" />

    <androidx.constraintlayout.widget.ConstraintLayout

        android:layout_width="match_parent"
        android:layout_height="518dp"
        android:layout_below="@+id/imageView12"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="0dp">


        <ImageView
            android:id="@+id/imageView24"
            android:layout_width="162dp"
            android:layout_height="224dp"
            android:layout_marginStart="24dp"
            android:layout_marginLeft="24dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rectangle01"
            android:contentDescription="@string/image"/>

        <ImageView
            android:id="@+id/imageView28"
            android:layout_width="42dp"
            android:layout_height="74dp"
            android:layout_marginTop="52dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView24"
            app:layout_constraintStart_toStartOf="@+id/imageView24"
            app:layout_constraintTop_toTopOf="@+id/imageView24"
            app:srcCompat="@drawable/cart"
            android:contentDescription="@string/image"/>

        <ImageView
            android:id="@+id/imageViewCart"
            android:layout_width="42dp"
            android:layout_height="74dp"
            android:layout_marginTop="52dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView24"
            app:layout_constraintStart_toStartOf="@+id/imageView24"
            app:layout_constraintTop_toTopOf="@+id/imageView24"
            app:srcCompat="@drawable/cart"
            android:contentDescription="@string/image"/>

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="64dp"
            android:fontFamily="@font/poppinssemibold"
            android:text="@string/items_added"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView24"
            app:layout_constraintHorizontal_bias="0.535"
            app:layout_constraintStart_toStartOf="@+id/imageView24"
            app:layout_constraintTop_toTopOf="@+id/imageView28" />

        <TextView
            android:id="@+id/addedTxtView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gontserratregular"
            android:text="@string/_10"
            android:textColor="@color/fuel_orange"
            app:layout_constraintEnd_toEndOf="@+id/textView8"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/textView8" />

        <ImageView
            android:id="@+id/imageView30"
            android:layout_width="162dp"
            android:layout_height="224dp"
            android:layout_marginStart="44dp"
            android:layout_marginLeft="44dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.157"
            app:layout_constraintStart_toEndOf="@+id/imageView24"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rectangle01"
            android:contentDescription="@string/image"/>

        <ImageView
            android:id="@+id/imageViewCard"
            android:layout_width="42dp"
            android:layout_height="74dp"
            android:layout_marginTop="52dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView30"
            app:layout_constraintStart_toStartOf="@+id/imageView30"
            app:layout_constraintTop_toTopOf="@+id/imageView30"
            app:srcCompat="@drawable/payments"
            android:contentDescription="@string/image"/>

        <TextView
            android:id="@+id/purchasedTxtView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="64dp"
            android:fontFamily="@font/poppinssemibold"
            android:text="@string/items_purchased"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView30"
            app:layout_constraintStart_toStartOf="@+id/imageView30"
            app:layout_constraintTop_toTopOf="@+id/imageViewCard" />

        <TextView
            android:id="@+id/item_purchased"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:fontFamily="@font/gontserratregular"
            android:text="@string/_10"
            android:textColor="@color/fuel_orange"
            app:layout_constraintEnd_toEndOf="@+id/purchasedTxtView"
            app:layout_constraintHorizontal_bias="0.495"
            app:layout_constraintStart_toStartOf="@+id/purchasedTxtView"
            app:layout_constraintTop_toBottomOf="@+id/imageViewCard" />

        <ImageView
            android:id="@+id/imageView37"
            android:layout_width="162dp"
            android:layout_height="224dp"
            android:layout_marginTop="188dp"
            android:layout_marginEnd="124dp"
            android:layout_marginRight="124dp"
            android:contentDescription="@string/image"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rectangle01" />

        <ImageView
            android:id="@+id/imageViewDelete"
            android:layout_width="42dp"
            android:layout_height="74dp"
            android:layout_marginTop="16dp"
            android:contentDescription="@string/image"
            app:layout_constraintEnd_toEndOf="@+id/imageView37"
            app:layout_constraintHorizontal_bias="0.491"
            app:layout_constraintStart_toStartOf="@+id/imageView37"
            app:layout_constraintTop_toBottomOf="@+id/imageView30"
            app:srcCompat="@drawable/delete" />

        <TextView
            android:id="@+id/deletedTxtView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:fontFamily="@font/gontserratregular"
            android:text="@string/_10"
            android:textColor="@color/fuel_orange"
            app:layout_constraintEnd_toEndOf="@+id/textView14"
            app:layout_constraintHorizontal_bias="0.494"
            app:layout_constraintStart_toStartOf="@+id/textView14"
            app:layout_constraintTop_toBottomOf="@+id/imageViewDelete" />

        <TextView
            android:id="@+id/textView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="72dp"
            android:fontFamily="@font/poppinssemibold"
            android:text="@string/items_deleted"
            android:textColor="@color/white"
            app:layout_constraintEnd_toEndOf="@+id/imageView37"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toStartOf="@+id/imageView37"
            app:layout_constraintTop_toTopOf="@+id/imageViewDelete" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/logoutAction"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/displayusername"
        android:layout_alignRight="@+id/displayusername"
        android:layout_alignBottom="@+id/displayusername"
        android:layout_marginEnd="-31dp"
        android:layout_marginRight="-31dp"
        android:layout_marginBottom="1dp"
        app:srcCompat="@drawable/ic_baseline_exit_to_app_24"
        tools:ignore="VectorDrawableCompat" />


</RelativeLayout>
